<h1>Billboards#new</h1>
<hr />
<%= form_with(model: @billboard, local: true, class: "ui form" ) do |f| %>
  <div class="field">
    <%= f.label :name %>
    <%= f.text_field :name, required: true, placeholder: "Billboard Name" %>
  </div>
  <%= f.submit nil, class: "ui blue button inverted" %>
<% end %>